          "name": "Create Package",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "pm.test('Package created', function() {",
                  "    pm.response.to.have.status(201);",
                  "});",
                  "",
                  "pm.test('Response has package data', function() {",
                  "    const response = pm.response.json();",
                  "    pm.expect(response.data.package).to.have.property('id');",
                  "    pm.expect(response.data.package).to.have.property('trackingNumber');",
                  "});",
                  "",
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.environment.set('packageId', response.data.package.id);",
                  "    pm.environment.set('trackingNumber', response.data.package.trackingNumber);",
                  "    console.log('Package ID saved:', response.data.package.id);",
                  "    console.log('Tracking Number saved:', response.data.package.trackingNumber);",
                  "}"
                ],
