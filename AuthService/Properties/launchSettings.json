{
  "profiles": {
    "AuthService": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "http://localhost:5001",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5081;http://localhost:5001"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "5081",
        "ASPNETCORE_HTTP_PORTS": "5080"
      },
      "publishAllPorts": true,
      "useSSL": true,
      "httpPort": 5001
    }
  }
}